[project]
	description = My repo
[access "refs/*"]
  read = group repo1-core
  owner = group repo1-ptl
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group repo1-core
	label-Code-Review = -2..+2 group repo1-ptl
	label-Verified = -2..+2 group repo1-ptl
	label-Workflow = -1..+1 group repo1-core
	label-Workflow = -1..+1 group repo1-ptl
	label-Workflow = -1..+0 group Registered Users
	submit = group repo1-ptl
	read = group repo1-core
    read = group Registered Users
[access "refs/meta/config"]
    read = group repo1-core
    read = group Registered Users
[receive]
	requireChangeId = true
[submit]
	mergeContent = false
	action = fast forward only
